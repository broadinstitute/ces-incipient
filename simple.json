{
  "commandline": "md5sum /job/inputs/sample.txt > /job/outputs/sample.md5.txt",
  "inputs": [
    {
      "cloud": "gs://broad-dsde-dev-public/sample.txt",
      "local": "/job/inputs/sample.txt"
    }
  ],
  "outputs": [
    {
      "cloud": "gs://broad-dsde-dev-public/sample.md5.txt",
      "local": "/job/outputs/sample.md5.txt"
    }
  ],
  "resources": {
    "cpus": 0.5,
    "mem": 1,
    "memunit": "GB",
    "disk": 1,
    "diskunit": "GB"

  },
  "docker": "broadinstitute/ces-incipient"
}
